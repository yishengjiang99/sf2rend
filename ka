import {mkdiv} from "./mkdiv/mkdiv";

let zone;
"Attenuation,VolEnv,Filter,LFO"
	.split(",")
	.map((keyword) => Object.entries(zone).filter(([k]) => k.includes(keyword)).map(zattrs => mkdiv("ul",
		zattrs.map(([k, v]) =>
			mkdiv("li", [
				mkdiv("label", [k, ":"]),
				mkdiv("code", [v]),
				mkdiv("input", {
					type: "range",
					value: v,
				}),
			])
		)
	)));